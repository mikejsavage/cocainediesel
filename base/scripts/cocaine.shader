textures/cocaine/s_lazery_blue
{
	qer_editorimage textures/cocaine/lazery_blue
	q3map_lightimage textures/cocaine/lazery_blue
	q3map_surfacelight 1200
	surfaceparm nolightmap
	surfaceparm nomarks
	surfaceparm nonsolid
	surfaceparm trans
	{
		map textures/cocaine/lazery_blue.tga
		blendfunc GL_ONE GL_ONE
		tcGen base
		tcmod scroll 2 -2.0
		tcmod rotate 120
	}
}

